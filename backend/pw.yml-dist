port: 8080

log-level: info

# Limit - how many secrets hold inside in-memory storage
storage-items-capacity: 1024

message-max-length: 1024

# Encrypted payload length > source message length
encrypted-message-max-length: 6128

# Look for `id` inside `locale.d` directory
locale-id: 'en'

redis-url: 'redis://cache:6379/'